                    <span class="info-box-icon bg-info elevation-1"><i class="fas fa-pen-to-square"></i></span>
                    <a asp-controller="Grade" asp-action="Index">
                        <div class="info-box-content">
                            <span class="info-box-text">Classes: @Model.Grades.Count</span>
                            <span class="info-box-number">
                                <small>My Classes: @Model.MyGrades.Count</small>
                            </span>
                        </div>
                    </a>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <div class="col-12 col-sm-6 col-md-3">
                <div class="info-box">
                    <span class="info-box-icon bg-primary elevation-1"><i class="fas fa-book"></i></span>
                    <a asp-controller="Subject" asp-action="Index">
                        <div class="info-box-content">
                            <span class="info-box-text">Subjects: @Model.Subjects.Count</span>
                            <span class="info-box-number">
                                <small>My Subjects: @Model.MySubjects.Count</small>
                            </span>
                        </div>
                    </a>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <div class="col-12 col-sm-6 col-md-3">
                <div class="info-box">
                    <span class="info-box-icon bg-info elevation-1"><i class="fas fa-question"></i></span>
                    <a asp-controller="Question" asp-action="Index">
                        <div class="info-box-content">
                            <span class="info-box-text">Questions: @Model.Questions.Count</span>
                            <span class="info-box-number">
                                <small>MyQuestions: @Model.MyQuestions.Count</small>
                            </span>
                        </div>
                    </a>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <!-- /.col -->
            <div class="col-12 col-sm-6 col-md-3">
                <div class="info-box mb-3">
                    <span class="info-box-icon bg-danger elevation-1"><i class="fas fa-clipboard"></i></span>
                    <a asp-controller="Paper" asp-action="UserPapers">
                        <div class="info-box-content">
                            <span class="info-box-text">Generated Papers: @Model.Papers.Count</span>
                            <span class="info-box-number"><small>My Papers: @Model.MyPapers.Count</small></span>
                        </div>
                    </a>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>

            <div class="col-12 col-sm-6 col-md-3">
                <div class="info-box mb-3">
                    <span class="info-box-icon bg-success elevation-1"><i class="fas fa-upload"></i></span>
                    <a asp-controller="SED" asp-action="Index">
                        <div class="info-box-content">
                            <span class="info-box-text">SED: @Model.Sed.Count</span>
                            <span class="info-box-number"><small>My SED Contributions: @Model.MySed.Count</small></span>
                        </div>
                    </a>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
        }
        @if (User.IsInRole(UserRoles.Student) || User.IsInRole(UserRoles.Admin))
        {
            <!-- /.col -->
            <div class="col-12 col-sm-6 col-md-3">
                <div class="info-box mb-3">
