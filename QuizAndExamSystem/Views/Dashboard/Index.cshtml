                    <a asp-controller="Quiz" asp-action="UserQuiz">
                        <div class="info-box-content">
                            <span class="info-box-text">Quizes taken: @Model.Quizes.Count</span>
                            <span class="info-box-number"><small>My quizes: @Model.MyQuizes.Count</small></span>
                        </div>
                    </a>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
        }
        <div class="col-12 col-sm-6 col-md-3">
            <div class="info-box mb-3">
                <span class="info-box-icon bg-danger elevation-1"><i class="fas fa-upload"></i></span>
                <a asp-controller="StudyMaterial" asp-action="UserIndex10th">
                    <div class="info-box-content">
                        <span class="info-box-text">Contributions: @Model.Uploads.Count</span>
                        <span class="info-box-number"><small>My Contributions: @Model.MyUploads.Count</small></span>
                    </div>
                </a>
                <!-- /.info-box-content -->
            </div>
            <!-- /.info-box -->
        </div>
