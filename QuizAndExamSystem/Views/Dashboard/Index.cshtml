                                        <span class="description-text">My Papers</span>
                                    </a>
                                </div>
                                <!-- /.description-block -->
                            </div>
                        }
                        @if (User.IsInRole(UserRoles.Admin) || (User.IsInRole(UserRoles.Student)))
                        {
                            <!-- /.col -->
                            <div class="col-sm-3 col-6">
                                <div class="description-block border-right">
                                    <a asp-controller="Quiz" asp-action="UserQuiz">
                                        <span class="description-percentage text-warning"><i class="fas fa-caret-left"></i>@Model.MyQuizes.Count()</span>
                                        <h5 class="description-header"> Take a Quiz</h5>
                                        <span class="description-text">My Quizes</span>
                                    </a>
                                </div>
                                <!-- /.description-block -->
                            </div>
                        }
                        <!-- /.col -->
                        @if (User.IsInRole(UserRoles.Admin) || (User.IsInRole(UserRoles.Teacher) && (User.HasClaim(c => c.Type == "Permission" && c.Value == UserPermissions.Moderator))))
                        {
                            <div class="col-sm-3 col-6">
                                <div class="description-block border-right">
                                    <a asp-controller="Question" asp-action="index">
                                        <span class="description-percentage text-success"><i class="fas fa-caret-up"></i> My questions:@Model.MyQuestions.Count()</span>
                                        <h5 class="description-header">Add New Question</h5>
                                        <span class="description-text">TOTAL Questions: @Model.Questions.Count()</span>
                                    </a>
                                </div>
                                <!-- /.description-block -->
                            </div>
                        }
                    </div>
                    <!-- /.row -->
                </div>
                <!-- /.card-footer -->
            </div>
            <!-- /.card -->
        </div>
        <!-- /.col -->
    </div>
